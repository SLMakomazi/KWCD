{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
